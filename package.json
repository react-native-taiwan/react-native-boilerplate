{
  "name": "RNBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "flux-constants": "^0.2.2",
    "immutable": "^3.8.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-native": "^0.51.0",
    "react-native-mock-render": "0.0.19",
    "react-native-router-flux": "^4.0.0-beta.27",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "babel-jest": "22.0.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "22.0.4",
    "jsdom-global": "3.0.2",
    "react-test-renderer": "16.0.0",
    "remote-redux-devtools": "^0.5.12",
    "sinon": "^4.1.4"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/configs/TestSetup.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "!node_modules/react-runtime"
    ],
    "moduleNameMapper": {
      ".+\\.(png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    }
  }
}
